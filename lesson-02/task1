#include <iostream>
#include <string>
using namespace std;
class Student
{
	public:
	string name;
	string hobby;
	int age;
};
class Animal
{
public:
	string species;
	string country;
	int individuals;
};

int main()
{
	Student abc;
	abc.name = "Hubert";
	abc.hobby = "volleyball";
	abc.age = 20;
	Animal efg;
	efg.species = "dog";
	efg.country = "Poland";
	efg.individuals = 1000000;
	cout << efg.species;
}
